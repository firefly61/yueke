.container {
  background: #f7d794;
  height: 100vh;
} 
.course-list {
  width: 100%;
  display: flex;
  flex-direction: row; 
  justify-content: flex-start;
  align-items: center;
  padding-left:60rpx;
  padding-top:30rpx;
  padding-bottom: 30rpx;
  background: #f1f2f6;
  border-bottom: 1px solid #dfe4ea;
}
.photo {
  width: 50px;
  height: 50px;
  border-radius: 50%;
  margin-right: 20rpx;
}
.content {
  display: flex;
  flex-direction: column;
  align-items: flex-start;
}
.course-name {
  font-size: 28rpx;
  color: #303952;
  margin-bottom: 12rpx;
}
.details {
  font-size: 24rpx;
  color: #596275;
}